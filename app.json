{
  "expo": {
    "name": "Weather app",
    "slug": "weatherApp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/weatherIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weatherIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.axie.weatherApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
